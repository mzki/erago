logfile = "erago.log"
loglevel = "info"
font = ""
width = 800
height = 600
history_line_count = 1024
testing_timeout_sec = 60

[game]
  [game.scene]
    CanAutoSave = true
  [game.save]
    SaveFileDir = "sav"
  [game.csv]
    Dir = "CSV"
    CharaPattern = "Chara/Chara*"
  [game.script]
    LoadDir = "ELA"
    LoadPattern = "init.lua"
    CallStackSize = 256
    RegistrySize = 5120
    IncludeGoStackTrace = true
    InfiniteLoopTimeoutSecond = 0
    ReloadFileChange = false
